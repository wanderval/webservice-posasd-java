<?xml version="1.0" encoding="UTF-8"?>
<definitions name="OperationsBpel" targetNamespace="http://j2ee.netbeans.org/wsdl/OperationsBpel/src/OperationsBpel"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
